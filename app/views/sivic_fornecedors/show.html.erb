<%if notice %>
  <div class="alert alert-success" role="alert">
    <%= notice %>
  </div>
<%end%>

<% content_for :titlePage do %>
  Exibir Fornecedor
<% end %>

<%= render 'layouts/panel' %>


<p>
  <strong>Nome Fornecedor:</strong>
  <%= @sivic_fornecedor.nome_fornecedor %>
</p>

<p>
  <strong>CNPJ:</strong>
  <%= @sivic_fornecedor.numr_cnpj %>
</p>

<p>
  <strong>CPF:</strong>
  <%= @sivic_fornecedor.numr_cpf %>
</p>

<p>
  <strong>Telefône:</strong>
  <%= @sivic_fornecedor.numr_telefone %>
</p>

<p>
  <strong>Nome Responsável:</strong>
  <%= @sivic_fornecedor.nome_responsavel %>
</p>

<p>
  <strong>Descrição Fornecedor:</strong>
  <%= @sivic_fornecedor.desc_fornecedor %>
</p>

<hr>
  Endereço</h4>

<p>
  <strong>Rua:</strong>
  <%= @sivic_fornecedor.sivic_endereco.DESC_Rua %>
</p>

<p>
  <strong>Complemento:</strong>
  <%= @sivic_fornecedor.sivic_endereco.DESC_Complemento %>
</p>


<p>
  <strong>CEP:</strong>
  <%= @sivic_fornecedor.sivic_endereco.NUMR_Cep %>
</p>

<p>
  <strong>Bairro:</strong>
  <%= @sivic_fornecedor.sivic_endereco.DESC_Bairro %>
</p>

<p>
  <strong>Ponto Referência:</strong>
  <%= @sivic_fornecedor.sivic_endereco.DESC_Pontoreferencia %>
</p>

<p>
  <strong>Cidade:</strong>
  <%= @sivic_fornecedor.sivic_endereco.sivic_cidade.nome_cidade %>
</p>

<p>
  <strong>Estado:</strong>
  <%= @sivic_fornecedor.sivic_endereco.sivic_cidade.sivic_estado.nome_estado %>
</p>


</div>
</div>

<div class="btn-group">

  <%= link_to edit_sivic_fornecedor_path(@sivic_fornecedor), :class => "btn btn-default" do %>
    <span class="glyphicon glyphicon-pencil"></span> Editar
  <% end %>
  <%= link_to sivic_fornecedors_path, :class => "btn btn-default" do %>
    <span class="glyphicon glyphicon-arrow-left"></span> Voltar
  <% end %>

</div>