<h1>Lista de Usuários</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Id</th>
      <th>E-mail</th>
      <th>Pessoa</th>
      <th>Permissão</th>
      <th>Último Login</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @sivic_users.each do |sivic_user| %>
      <tr>
        <td><%= sivic_user.id %></td>
        <td><%= sivic_user.email %></td>
        <td><%= sivic_user.sivic_pessoa.NOME_pessoa %></td>
        <td><%= sivic_user.role %></td>
        <td><%= sivic_user.last_sign_in_at %></td>
        <td><%= link_to 'Exibir', User %></td>
        <td><%#= link_to 'Editar', edit_sivic_estado_path(sivic_estado) %></td>
        <td><%#= link_to 'Excluir', sivic_estado, method: :delete, data: { confirm: 'Deseja exluir o registro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Registro', new_user_path %>
