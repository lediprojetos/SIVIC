<h1>Lista de Inscrições</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Evento</th>
      <th>Valor Pago</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @sivic_parteventos.each do |sivic_partevento| %>
      <tr>
        <td><%= sivic_partevento.sivic_pessoa.NOME_pessoa %></td>
        <td><%= sivic_partevento.sivic_evento.DESC_evento %></td>
        <td><%= sivic_partevento.sivic_movimentofinanceiro.VALR_movimento %></td>
        <td><%= link_to 'Exibir', sivic_partevento %></td>
        <td><%= link_to 'Editar', edit_sivic_partevento_path(sivic_partevento) %></td>
        <td><%= link_to 'Deletar', sivic_partevento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nova Inscrição', new_sivic_partevento_path %>
